<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coopbuy.mall">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.CoopBuyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ui.mainpage.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.module.test.activity.TestActivity" />
        <activity android:name=".ui.module.test.activity.SecondActivity" />
        <activity android:name=".ui.module.test.activity.TabLayoutActivity"/>
    </application>

</manifest>